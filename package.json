{
  "license": "MIT",
  "name": "11ty-hbs-webpack",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/uuki/11ty-hbs-webpack"
  },
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm-run-all -l clean --parallel dev:*",
    "dev:webpack": "webpack --watch",
    "dev:eleventy": "eleventy --serve",
    "build": "cross-env NODE_ENV=production npm-run-all -l clean build:eleventy build:webpack",
    "build:webpack": "webpack",
    "build:eleventy": "eleventy",
    "build:font": "svg2font --config ./svg2font.config.js",
    "analyze": "npm-run-all build analyze:*",
    "analyze:js": "source-map-explorer _site/**/*.js _site/**/*.js.map --no-border-checks",
    "format": "prettier --write **/*.{js,s?css,yml} !_site",
    "lint:js": "./node_modules/.bin/eslint \"src/assets/scripts/**/*.js\"",
    "lint:styles": "./node_modules/.bin/stylelint \"src/assets/styles/**/*.scss\"",
    "fix:js": "yarn lint:js --fix",
    "fix:style": "yarn lint:style --fix",
    "clean": "rimraf _site"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-img": "^0.8.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@babel/cli": "^7.13.8",
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.8",
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@uuki/svg2font": "^0.0.9",
    "babel-loader": "^8.2.2",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "date-fns": "^2.16.1",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "handlebars-helpers": "^0.10.0",
    "html-minifier": "^4.0.0",
    "import-glob-loader": "^1.1.0",
    "js-yaml": "^4.0.0",
    "license-info-webpack-plugin": "^3.0.0",
    "markdown-it": "^12.0.3",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-bracketed-spans": "^1.0.1",
    "mini-css-extract-plugin": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.7.1",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "promised-handlebars": "^2.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "source-map-explorer": "^2.5.1",
    "stylelint": "13.11.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.19.0",
    "stylelint-webpack-plugin": "^2.2.1",
    "svg-sprite": "^1.5.0",
    "vinyl": "^2.2.1",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-manifest-plugin": "^3.0.0"
  },
  "dependencies": {}
}
